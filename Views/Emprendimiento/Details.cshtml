@model CasaOrtApp.Models.Emprendimiento

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>Emprendimiento</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Calle
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Street)
        </dd>
        <dt class = "col-sm-2">
            Numero
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Number)
        </dd>
        <dt class = "col-sm-2">
            Tamaño(Mts)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Size)
        </dd>
        <dt class = "col-sm-2">
            Jardin
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Garden)
        </dd>
        <dt class = "col-sm-2">
            Pileta
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.SwimmingPool)
        </dd>
        <dt class = "col-sm-2">
            Foto
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Path)
        </dd>
        <dt class = "col-sm-2">
            Precio(U$s)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Price)
        </dd>
        <dt class = "col-sm-2">
            Etapa
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.etapa)
        </dd>
        <dt class = "col-sm-2">
            Fecha de Publicacion
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ReleaseDate)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.Id">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
